<%- include('partials/header') %>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input type="text" class="form-control" id="postTitle" placeholder="Enter Title" name="postTitle">
  </div>
  <div class="form-group">
    <label for="postData">Post</label>
    <textarea class="form-control" id="postData" rows="3" style="margin: 0px 2px 0px 0px; width: 1140px; height: 81px;" name="postData"></textarea>
  </div>
  <div class="form-group">
    <input type="submit" name="btnadd" class="btn btn-primary" value="Publish">
  </div>
</form>
<%- include('partials/footer') %>
